// CR 1361 Rule changes- Start
rule "Chip_Summary_1_1A"
salience 49598
activation-group "Chip_Summary_1_AT"
agenda-group "Chip_Summary_1_AG"
	when
		//<CONDITION>
		$eDeterminationfacts : EEDeterminationRuleFacts(!isReceivingNursingOrFosterCareReferral(ProgramCode.MEDICAID),
														isProgramStatusInEligible(ProgramCode.CHIP),
														chipTerminatedDueToNyshipInd,
														isProposedElgForRenewal);
	then
		//<ACTIONS>
		logger.debug("Rule : Chip_Summary_1_1A : Begin");
		$eDeterminationfacts.getElgRulesIndicatorForProgram(ProgramCode.CHIP).setManualRenewalInd(Boolean.TRUE);
		$eDeterminationfacts.updateProgram(ProgramCode.CHIP, program_status.INELIGIBLE, ProgramReasonCode.MRR);	
		$eDeterminationfacts.updateRuleExec(ProgramCode.CHIP, "Chip_Summary_1_1A");
		drools.setFocus("Chip_Summary_3_AG");
		logger.debug("Rule : Chip_Summary_1_1A : End");
end


