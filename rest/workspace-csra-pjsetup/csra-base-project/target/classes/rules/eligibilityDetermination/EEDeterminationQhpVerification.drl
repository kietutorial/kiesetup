rule "MAIN_QHP"
salience 100000
	when
		$eDeterminationfacts : EEDeterminationRuleFacts();
	then
		logger.debug("Rule : MAIN_QHP : Begin");
		$eDeterminationfacts.updateRuleExec(ProgramCode.QHP, "MAIN_QHP");
		$eDeterminationfacts.setRuleExecCompletionStep(ProgramCode.QHP, HXConstants.RULE_EXEC_STEP_INIT_ACK);
		drools.setFocus("QHP_STALE_1_AG");
		logger.debug("Rule : MAIN_QHP : End");
end


//CR1606
rule "QHP_VERIFY_STALE_ELG_SEC_01"
salience 95050
activation-group "QHP_STALE_1_AT"
agenda-group "QHP_STALE_1_AG"
	when
		$eDeterminationfacts : EEDeterminationRuleFacts(!isStaleEligibility);
		//eval(true);		
	then
		logger.debug("Rule : QHP_VERIFY_STALE_ELG_SEC_01 : Begin");
		$eDeterminationfacts.updateRuleExec(ProgramCode.QHP, "QHP_VERIFY_STALE_ELG_SEC_01");
		drools.setFocus("Qhp_Deceased_1_AG");
		logger.debug("Rule : QHP_VERIFY_STALE_ELG_SEC_01 : End");
end

rule "QHP_VERIFY_STALE_ELG_SEC_02"
salience 95060
activation-group "QHP_STALE_1_AT"
agenda-group "QHP_STALE_1_AG"
	when
		$eDeterminationfacts : EEDeterminationRuleFacts(isStaleEligibility,!isLSCThroughOnlineForStaleEligibility);
		//eval(true);		
	then
		logger.debug("Rule : QHP_VERIFY_STALE_ELG_SEC_02 : Begin");
		$eDeterminationfacts.updateRuleExec(ProgramCode.QHP, "QHP_VERIFY_STALE_ELG_SEC_02");
		$eDeterminationfacts.updateProgram(ProgramCode.QHP, program_status.INELIGIBLE, ProgramReasonCode.STALE_MEM);// set program as Ineligble with reason code
		$eDeterminationfacts.skipProgramExecution(ProgramCode.QHP, HXConstants.BOLTRUE); // Set Flag to stop further execution of drls for this program
		$eDeterminationfacts.setRuleExecCompletionStep(ProgramCode.QHP, HXConstants.RULE_EXEC_STEP_SEC1_03);
		//drools.setFocus("QHP_REL_1_AG");
		logger.debug("Rule : QHP_VERIFY_STALE_ELG_SEC_02 : End");
end

rule "QHP_VERIFY_STALE_ELG_SEC_03"
salience 95070
activation-group "QHP_STALE_1_AT"
agenda-group "QHP_STALE_1_AG"
	when
		$eDeterminationfacts : EEDeterminationRuleFacts(isStaleEligibility,isLSCThroughOnlineForStaleEligibility);
		//eval(true);		
	then
		logger.debug("Rule : QHP_VERIFY_STALE_ELG_SEC_03 : Begin");
		$eDeterminationfacts.updateRuleExec(ProgramCode.QHP, "QHP_VERIFY_STALE_ELG_SEC_03");
		$eDeterminationfacts.clearStaleFlag();
		drools.setFocus("Qhp_Deceased_1_AG");
		logger.debug("Rule : QHP_VERIFY_STALE_ELG_SEC_03 : End");
end
//CR1606 code changes end


