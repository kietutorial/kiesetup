//Main Rule


rule "MAIN_MEDICAID_OVERRIDE"
salience 100000
	when
		$eDeterminationfacts : EEDeterminationRuleFacts();
		//eval(true);		
	then
		logger.debug("Rule : MAIN_MEDICAID_OVERRIDE : Begin");
		$eDeterminationfacts.updateRuleExec(ProgramCode.MEDICAID, "MAIN_MEDICAID_OVERRIDE");
		$eDeterminationfacts.setRuleExecCompletionStep(ProgramCode.MEDICAID, HXConstants.RULE_EXEC_STEP_OVERRIDE_ACK);
		drools.setFocus("MAIN_MEDICAID_OVERRIDE_STALE_1_AG");
		logger.debug("Rule : MAIN_MEDICAID_OVERRIDE : End");
end


//CR1606
rule "MAIN_MEDICAID_OVERRIDE_STALE_ELG_SEC_01"
salience 95050
activation-group "MAIN_MEDICAID_OVERRIDE_STALE_1_AT"
agenda-group "MAIN_MEDICAID_OVERRIDE_STALE_1_AG"
	when
		$eDeterminationfacts : EEDeterminationRuleFacts(!isStaleEligibility);
		//eval(true);		
	then
		logger.debug("Rule : MAIN_MEDICAID_OVERRIDE_STALE_ELG_SEC_01 : Begin");
		$eDeterminationfacts.updateRuleExec(ProgramCode.MEDICAID, "MAIN_MEDICAID_OVERRIDE_STALE_ELG_SEC_01");
		drools.setFocus("Medicaid_OVR_GENERAL_1_AG");
		logger.debug("Rule : MAIN_MEDICAID_OVERRIDE_STALE_ELG_SEC_01 : End");
end

rule "MAIN_MEDICAID_OVERRIDE_STALE_ELG_SEC_02"
salience 95060
activation-group "MAIN_MEDICAID_OVERRIDE_STALE_1_AT"
agenda-group "MAIN_MEDICAID_OVERRIDE_STALE_1_AG"
	when
		$eDeterminationfacts : EEDeterminationRuleFacts(isStaleEligibility,!isLSCThroughOnlineForStaleEligibility);
		//eval(true);		
	then
		logger.debug("Rule : MAIN_MEDICAID_OVERRIDE_STALE_ELG_SEC_02 : Begin");
		$eDeterminationfacts.updateRuleExec(ProgramCode.MEDICAID, "MAIN_MEDICAID_OVERRIDE_STALE_ELG_SEC_02");
		$eDeterminationfacts.updateProgram(ProgramCode.MEDICAID, program_status.INELIGIBLE, ProgramReasonCode.STALE_MEM);// set program as Ineligble with reason code
		$eDeterminationfacts.skipProgramExecution(ProgramCode.MEDICAID, HXConstants.BOLTRUE); // Set Flag to stop further execution of drls for this program
		$eDeterminationfacts.setRuleExecCompletionStep(ProgramCode.MEDICAID, HXConstants.RULE_EXEC_STEP_OVERRIDE_COMPLETE);
		//drools.setFocus("Medicaid_Dup_Cov_1_AG");
		logger.debug("Rule : MAIN_MEDICAID_OVERRIDE_STALE_ELG_SEC_02 : End");
end

rule "MAIN_MEDICAID_OVERRIDE_STALE_ELG_SEC_03"
salience 95070
activation-group "MAIN_MEDICAID_OVERRIDE_STALE_1_AT"
agenda-group "MAIN_MEDICAID_OVERRIDE_STALE_1_AG"
	when
		$eDeterminationfacts : EEDeterminationRuleFacts(isStaleEligibility,isLSCThroughOnlineForStaleEligibility);
		//eval(true);		
	then
		logger.debug("Rule : MAIN_MEDICAID_OVERRIDE_STALE_ELG_SEC_03 : Begin");
		$eDeterminationfacts.updateRuleExec(ProgramCode.MEDICAID, "MAIN_MEDICAID_OVERRIDE_STALE_ELG_SEC_03");
		$eDeterminationfacts.clearStaleFlag();
		drools.setFocus("Medicaid_OVR_GENERAL_1_AG");
		logger.debug("Rule : MAIN_MEDICAID_OVERRIDE_STALE_ELG_SEC_03 : End");
end
//CR1606 code changes end

