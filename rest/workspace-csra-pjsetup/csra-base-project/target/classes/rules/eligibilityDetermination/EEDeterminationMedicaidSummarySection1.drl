rule "Medicaid_Summary_1_4"
salience 49670
activation-group "Medicaid_Summary_1_AT"
agenda-group "Medicaid_Summary_1_AG"
	when
		//<CONDITION>
		$eDeterminationfacts : EEDeterminationRuleFacts(inMemTerminateCovReleaseCode,
														!IncarcerationReleaseEnum.REL_8.Value().equals(inMemReleaseCode),
														IncarcerationReleaseEnum.REL_3.Value().equals(inMemReleaseCode),
														HXConstants.NO.equals(nonIncarVerificationReceived),
														!getIsNonIncarClockExistsAndUserDoesntAgree());
	then
		//<ACTIONS>
		logger.debug("Rule : Medicaid_Summary_1_4 : Begin");
		$eDeterminationfacts.updateRuleExec(ProgramCode.MEDICAID, "Medicaid_Summary_1_4");
			
		$eDeterminationfacts.updateProgramSpecial(ProgramCode.MEDICAID, program_status.INELIGIBLE, ProgramReasonCode.INE_REL_TO_IMMIG);
		$eDeterminationfacts.getElgRulesIndicatorForProgram(ProgramCode.MEDICAID).setCoverageEndIndicator($eDeterminationfacts.getInMemRelSplCovEndDateInd());
		$eDeterminationfacts.getElgRulesIndicatorForProgram(ProgramCode.MEDICAID).setPrevElgEndDateForTermRel($eDeterminationfacts.getInMemRelSplCalcEndDate());
		$eDeterminationfacts.getElgRulesIndicatorForProgram(ProgramCode.MEDICAID).setIncarReleaseCovTerminatedInd(Boolean.TRUE);
		$eDeterminationfacts.getElgRulesIndicatorForProgram(ProgramCode.MEDICAID).setMedicaidNonCompInd(Boolean.TRUE);
		$eDeterminationfacts.updateDOCCSIndInfo(ProgramCode.MEDICAID, DoccsType.PRE_RELEASED.name(), Boolean.FALSE);
		$eDeterminationfacts.updatePostPartumSaveDate(ProgramCode.MEDICAID, null);
		drools.setFocus("Medicaid_Summary_END_AG");		
		logger.debug("Rule : Medicaid_Summary_1_4 : End");
end

